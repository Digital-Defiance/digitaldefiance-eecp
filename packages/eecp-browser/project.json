{
  "name": "@digitaldefiance/eecp-browser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eecp-browser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc",
        "cwd": "packages/eecp-browser"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest --run",
        "cwd": "packages/eecp-browser"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": ["type:library", "scope:browser"],
  "implicitDependencies": [
    "@digitaldefiance/eecp-protocol",
    "@digitaldefiance/eecp-crypto",
    "@digitaldefiance/eecp-crdt",
    "@digitaldefiance/eecp-client"
  ]
}
